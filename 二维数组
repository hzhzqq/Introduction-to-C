int main()
{
    int i,j;
    int arr[][4]={{1},{2},{3}};//其中二维数组的行可动态，列不可动态
    for(i=0;i<3;i++)
    {
        for(j=0;j<4;j++)
        {
            printf("%-3d",arr[i][j]);
        } 
        printf("\n");       
    }
    return 0;
}
